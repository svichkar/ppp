<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{ISO8601} [%-5p][%t][%c] - %m%n"/>
        </Console>
    </appenders>
    <loggers>
        <Root level="TRACE">
            <appender-ref ref="Console"/>
        </Root>
    </loggers>
</configuration>